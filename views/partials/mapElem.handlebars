<g data-neighbor="{{neighbor}}" data-troops="{{troops}}" data-owner="{{owner}}" data-name="{{name}}" fill="none" id="{{id}}" stroke="#000000" class="{{class}}">

{{#each path}}
    <path d="{{this}}"/>
{{/each}}

<text class="troop-count" fill="#000000" font-family="Serif" font-size="24" id="{{id}}-troops" stroke="#000000" stroke-width="0" text-anchor="middle" x="{{labelX}}" xml:space="preserve" y="{{labelY}}">{{troops}}</text>

</g>