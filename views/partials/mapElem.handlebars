<g data-neighbor="{{neighbor}}" data-troops="{{troops}}" data-owner="{{owner}}" data-name="{{name}}" fill="none" id="{{id}}" stroke="#000000" class="{{class}}">

{{#each path}}
    <path d="{{this}}"/>
{{/each}}

<text class="troop-count" fill="#000000" id="{{id}}-troops" text-anchor="middle" x="{{labelX}}" xml:space="preserve" y="{{labelY}}">{{troops}}</text>

</g>